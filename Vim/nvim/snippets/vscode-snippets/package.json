{
  "name": "vscode-snippets",
  "author": "beauwilliams",
  "engines": {
    "vscode": "^1.11.0"
  },
  "contributes": {
    "snippets": [
      {
        "language": [
          "plaintext",
          "markdown",
          "tex",
          "html",
          "global",
          "all"
        ],
        "path": "./snippets/global.json"
      },
      {
        "language": "json",
        "path": "./snippets/json.json"
      }
    ]
  }
}
